onents/Search';\nimport Alert from './components/Alert';\nimport Weather from './components/Weather';\nimport SearchCurrentLocation from './components/SearchCurrentLocation';\n\nimport { setAlert } from './store/actions/alertActions';\nimport { setError } from './store/actions/weatherActions';\nimport { WeatherData } from './store/types';\n\n\n\n\nconst App: FC = () => {\n  const dispatch = useDispatch();\n  const weatherData = useSelector((state: RootState) => state.weather.data);\n  const loading = useSelector((state: RootState) => state.weather.loading);\n  const error = useSelector((state: RootState) => state.weather.error);\n  const alertMsg = useSelector((state: RootState) => state.alert.message);\n\n  return (\n    <div\n      className={\n        weatherData\n          ? weatherData.main.temp > 18\n            ? \"App-hot\"\n            : \"App-cold\"\n          : \"App\"\n      }\n    >\n      <div className=\"has-text-centered\">\n        <h1 className=\"has-text-left-tablet has-text-black-ter\tis-size-1 px-6 pt-6\">Weather App</h1>\n        <Search title=\"Enter city name and press search button\" />\n        <SearchCurrentLocation title=\"or press the compass to get your current location\" />\n\n        {loading ? <h2 className=\"is-size-3 py-2\">Loading...</h2> : weatherData && <Weather data={weatherData} />}\n\n        {alertMsg && <Alert message={alertMsg} onClose={() => dispatch(setAlert(''))} />}\n        {error && <Alert message={error} onClose={() => dispatch(setError())} />}\n      </div>\n    </div>\n  );\n}\n\nexport default App;","/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/store/actions/weatherActions.ts",[],"/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/store/actions/alertActions.ts",[],"/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/components/Alert.tsx",[],"/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/components/Search.tsx",[],"/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/components/Weather.tsx",["59"],"import React, { FC } from 'react';\nimport { WeatherData } from '../store/types';\nimport Moment from 'react-moment';\n\ninterface WeatherProps {\n  data: WeatherData;\n}\n\nconst Weather: FC<WeatherProps> = ({ data }) => {\n\n  const currentDate = new Date;\n\n  return(\n    <section className=\"section pt-0\">\n      <div className=\"container\">\n        <h1 className=\"title has-text-centered pb-3\" style={{marginBottom: 20}}>{data.name} - {data.sys.country}</h1>\n          <h1 className=\"subtitle\" ><Moment format=\"ddd Do MMMM YYYY\">{currentDate}</Moment></h1>\n          <div className=\"level-item has-text-centered pt-2\">\n            <div>\n              <p className=\"heading\">{data.weather[0].description}</p>\n              <p className=\"title\"><img src={`http://openweathermap.org/img/wn/${data.weather[0].icon}.png`} alt=\"\"/></p>\n            </div>\n          </div>\n        <div className=\"level\" style={{alignItems: 'flex-start'}}>\n          <div className=\"level-item has-text-centered\">\n            <div>\n              <p className=\"heading\">temp</p>\n              <div className=\"title\">\n                <p className=\"mb-2\">{data.main.temp}<sup>&#8451;</sup></p>\n              </div>\n            </div>\n          </div>\n          <div className=\"level-item has-text-centered\" >\n            <div>\n              <p className=\"heading\">feels like</p>\n              <div className=\"title\">\n                <p className=\"mb-2\">{data.main.feels_like}<sup>&#8451;</sup></p>\n              </div>\n            </div>\n          </div>\n          <div className=\"level-item has-text-centered\">\n            <div>\n              <p className=\"heading\">Min temperature</p>\n              <div className=\"title\">\n                <p className=\"mb-2\">{data.main.temp_min}<sup>&#8451;</sup></p>\n              </div>\n            </div>\n          </div>\n          <div className=\"level-item has-text-centered\">\n            <div>\n              <p className=\"heading\">Max temperature</p>\n              <div className=\"title\">\n                <p className=\"mb-0\">{data.main.temp_max}<sup>&#8451;</sup></p>\n              </div>\n            </div>\n          </div>\n\n        </div>\n      </div>\n    </section>\n  );\n}\n\nexport default Weather;","/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/store/types.ts",[],["60","61"],"/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/store/index.ts",[],"/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/store/reducers/alertReducer.ts",["62"],"import { AlertState, AlertAction, SET_ALERT } from \"../types\"\n\nconst initialState: AlertState = {\n    message: ''\n}\n\nexport default (state = initialState, action: AlertAction): AlertState => {\n    switch(action.type) {\n        case SET_ALERT:\n            return {\n                message: action.payload\n            }\n        default:\n            return state;\n    }\n}","/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/store/reducers/weatherReducer.ts",["63"],"import { WeatherState, WeatherAction, GET_WEATHER, SET_LOADING, SET_ERROR } from \"../types\";\n\nconst initialState: WeatherState = {\n    data: null,\n    loading: false,\n    error: ''\n}\n\nexport default (state = initialState, action: WeatherAction): WeatherState => {\n  switch(action.type) {\n    case GET_WEATHER:\n      return {\n        data: action.payload,\n        loading: false,\n        error: ''\n      }\n    case SET_LOADING:\n      return {\n        ...state,\n        loading: true\n      }\n    case SET_ERROR: \n      return {\n        ...state,\n        error: action.payload,\n        loading: false\n      }\n    default: \n      return state;\n  }\n}","/home/facu/code/MrFacundo/react/weatherapp3/weatherapp3/src/components/SearchCurrentLocation.tsx",[],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","severity":1